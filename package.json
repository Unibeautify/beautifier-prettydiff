{
  "name": "@unibeautify/beautifier-prettydiff",
  "version": "0.1.1",
  "description": "Pretty Diff beautifier for Unibeautify",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "tsc",
    "ava": "ava --verbose",
    "test": "npm-run-all compile:ts ava",
    "compile:ts": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Unibeautify/beautifier-prettydiff.git"
  },
  "keywords": [
    "pretty",
    "diff",
    "unibeautify",
    "beautify",
    "beautifier"
  ],
  "author": "Glavin Wiechert",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Unibeautify/beautifier-prettydiff/issues"
  },
  "homepage": "https://github.com/Unibeautify/beautifier-prettydiff#readme",
  "dependencies": {
    "prettydiff2": "^2.2.7",
    "unibeautify-beautifier": "^0.1.3"
  },
  "devDependencies": {
    "@types/node": "^8.5.7",
    "ava": "^0.24.0",
    "npm-run-all": "^3.1.0",
    "typescript": "^2.6.2",
    "unibeautify": "^0.2.1"
  },
  "providedServices": {
    "unibeautify": {
      "versions": {
        "1.0.0": "provideBeautifier"
      }
    }
  },
  "package-deps": [
    "atom-beautify"
  ]
}
